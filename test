// Script file for testing

function test_function_call(a) {
    return a + 5;
}

function test() {
    a = test_function_call(15);
    for (i = 0; i < 10; i += 1) {
        a = a + 1;
    }
    while (a < 50) {
        a = a + 1;
    }
    if (a == 1) {
        return "one";
    } else if (a == 2) {
        return "two";
    } else if (a == 3) {
        return "three";
    } else {
        a -= 5;
        return i;
    }
}
