// Script file for testing

MAP = new_instance("java.util.HashMap", new_array("java.lang.Object", 0));

function fibbonacci(count) {
    n = new_array("java.lang.Integer", count);
    n[0] = 0;
    n[1] = 1;

    // Loop starts from 2 because 0 and 1 are already printed
    for (i = 2; i < count; i++) {
        n[i] = n[i - 1] + n[i - 2];
    }

    return n;
}

function factorial_i(n) {
    rv = 1;
    while (n != 1) {
        rv *= n--;
    }
    return rv;
}

function factorial_r(n) {
    if (n == 1) return 1;
    else return n * factorial_i(--n);
}

function main(hook) {
    MAP.put("key", "value");
    println("Map Size: " + MAP.size());
    println("Stored Value: " + MAP.get("key"));

    array = fibbonacci(10);
    for (i = 0; i < 10; i++) {
        if (i < 9) print(array[i] + " ");
        else println(array[i]);
    }
}
