// Script file for testing

function fibbonacci(hook, count) {
    n1 = 0;
    n2 = 1;
    n3 = 0;

    // Printing 0 and 1
    hook.out.println(n1);
    hook.out.println(n2);

    // Loop starts from 2 because 0 and 1 are already printed
    for (i = 2; i < count; i++) {
        n3 = n1 + n2;
        hook.out.println(n3);
        n1 = n2;
        n2 = n3;
    }
}

function factorial(n) {
    if (n == 1.0) return 1.0;
    else return n * factorial(n - 1);
}

function main(hook) {
//  fibbonacci(hook, 10);

    n = 5;
    hook.out.println("Factorial of " + n + " equals " + factorial(n));

//  hook.thread.sleep(1000);
//  hook.out.println("Slept for 1000 ms");

    b = true;
    b = !b;
    a = +1;
    hook.out.println(a);
}
